.contentview .title{
  height:90rpx;
  line-height:90rpx;
  text-align: center;
  font-size:30rpx;
  font-weight:600;
  color:#E7011B;
  background:#e5e5e5;
}
.dailyExam{
  margin:30rpx 15rpx;
}
.dailyExam>view{
  width:46%;
  float:left;
  height:200rpx;
  line-height: 200rpx; 
  position: relative;
  margin:0 2%;
}
.dailyExam .bgImg{
  width: 100%;
  height: 100%;
  z-index: -1;
  position: absolute;
  left: 0;
  top: 0;
}
.dailyExam .logoImg{
  width:140rpx;
  height:140rpx;
  position: absolute;
  right:0;
  margin-right:10rpx;
  top:50%;
  margin-top:-70rpx;
}
.dailyExam text{
  text-align: left;
  margin-left:36rpx;
}

