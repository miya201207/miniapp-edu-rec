/**index.wxss**/
.containers{
  width:100%;
  height:100%;
}
.swiper-banner {
  height: 307rpx;
  width: 100%;
}

.banner-img {
  height: 307rpx;
  width: 100%;
}

.notice {
  margin-top: 12rpx;
  width: 96%;
  margin:0 2%;
  height: 132rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 4rpx 15rpx 1rpx rgba(0, 0, 0, 0.2);
  border-radius: 20rpx;
}

.notice-img {
  margin-left: 36rpx;
  width: 64rpx;
  height: 72rpx;
}

.notice-line {
  margin-left: 21rpx;
  height: 81rpx;
  width: 2rpx;
  background: #dcdcdc;
}

.point {
  margin-left: 23rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 70rpx;
  justify-content: space-between;
}

.point-up {
  width: 10rpx;
  height: 10rpx;
  background: #e32516;
  border-radius: 50%;
}

.point-down {
  width: 10rpx;
  height: 10rpx;
  background: #f00615;
  border-radius: 50%;
}

.swiper-notice {
  margin-left: 20rpx;
  height: 100rpx;
  width: 100%;
  margin-right: 26rpx;
}

.notice-item {
  display: flex;
  flex-direction: column;
  align-items: left;
  justify-content: space-between;
}

.notice-text-up {
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  height: 50rpx;
  line-height: 50rpx;
  font-weight: 400;
  color: #E32516;
  overflow: hidden;
}

.notice-text-down {
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #FF8C00;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
}

.navigation {
  padding: 25rpx 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  background: #F9F9F9;
  color:#FF000000;
}
.navigations{
  margin: 50rpx 0; 
  padding:0;
  background: #fff;
}
.navigation-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.navigation-img {
  height: 110rpx;
  width: 110rpx;
}
.navigations-img {
  height: 92rpx;
  width: 92rpx;
}
.navigation-text {
  margin-top: 13rpx;
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.divider-line {
  margin-top: 20rpx;
  width: 100%;
  height: 10rpx;
  background: #f3f3f3;
}

.announcement {
  margin-top: 20rpx;
  width: 100%;
  display: flex;
  height: 70rpx;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom:1px solid #D9D9D9;
}

.announcement-title {
  margin-left: 20rpx;
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: bold;
  color: rgb(220,11,10);
}
.announcement-title>i{
  display:inline-block;
  vertical-align: middle;
  height:36rpx;
  width:6rpx;
  background:rgb(220,11,10);
  margin-right:20rpx;
}
.announcement-more {
  margin-right: 20rpx;
  font-size: 32rpx;
  font-family: Source Han Sans CN;
  color: rgba(153, 153, 153, 1);
}

.scrollview {
  height: 700rpx;
}
.scroll-item {
  border-bottom: 1rpx solid #f3f3f3;
  padding: 20rpx 0;
  text-align: left;
  width: 94%;
  margin: 0 3%;
}

.scroll-title {
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
 
}
.clockIn{
  width:96%;
  margin:30rpx 2% 50rpx 2%;

}
.clockIn>view{
  float:left;
  width:46%;
  margin:2%;
  text-align: center;
  height:170rpx;
  line-height: 170rpx;
  border-radius: 10rpx;
}
.clockIn>view>image{
  width:100%;
  height:100%;
}
.toClock{
  position: fixed;
  bottom:0rpx;
  background:red;
  width:140rpx;
  height:140rpx;
  left:50%;
  margin-left:-70rpx;
  z-index: 1000000;
}
.hr{
  background:#F9F9F9;
  height:10rpx;
  width:100%;
}